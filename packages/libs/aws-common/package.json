  "dependencies": {
    "@aws-sdk/client-s3": "3.48.0",
    "@aws-sdk/client-sqs": "3.48.0",
    "@sls-next/core": "link:../core"
  },
  "devDependencies": {
    "@types/node": "17.0.12",
    "rimraf": "3.0.2",
    "typescript": "4.5.5"
